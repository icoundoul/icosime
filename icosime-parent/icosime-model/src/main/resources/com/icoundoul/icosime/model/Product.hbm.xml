<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="com.icoundoul.icosime.model.Product" table="product">
		<id
			column="id"
			name="Id"
			type="integer"
		>
			<generator class="native" />
		</id>
		<property
			column="name"
			length="250"
			name="Name"
			not-null="true"
			type="string"
		 />
		 <property
			column="marque"
			length="250"
			name="Marque"
			not-null="true"
			type="string"
		 />
	</class>
</hibernate-mapping>
