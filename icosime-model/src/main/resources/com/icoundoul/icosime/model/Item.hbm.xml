<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >

<hibernate-mapping>
	<class name="com.icoundoul.icosime.model.Item" table="item">
		<id
			column="id"
			name="Id"
			type="integer"
		>
			<generator class="increment" />
		</id>
		<property
			column="description"
			length="250"
			name="Description"
			not-null="true"
			type="string"
		 />
		<property
			column="name"
			length="250"
			name="Name"
			not-null="true"
			type="string"
		 />
		
	</class>
</hibernate-mapping>
